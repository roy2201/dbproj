<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: DFDFDF;" stylesheets="@mystyle.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.taxiandrentapp.SignupController">
   <children>
      <Button id="create" fx:id="btnconfirm" layoutX="228.0" layoutY="301.0" mnemonicParsing="false" onAction="#ConfirmSignup" prefHeight="34.0" prefWidth="141.0" text="Create Account">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Label fx:id="info" layoutX="186.0" layoutY="347.0" prefHeight="18.0" prefWidth="225.0" />
      <Button id="back" layoutX="28.0" layoutY="347.0" mnemonicParsing="false" onAction="#signInPage" prefHeight="31.0" prefWidth="54.0" text="back">
         <font>
            <Font size="14.0" />
         </font></Button>
      <VBox alignment="CENTER" layoutX="213.0" layoutY="41.0" prefHeight="178.0" prefWidth="173.0" spacing="10.0">
         <children>
            <TextField fx:id="txtFirstName" promptText="firstname">
               <font>
                  <Font name="Arial Rounded MT Bold" size="14.0" />
               </font></TextField>
            <TextField fx:id="txtLastName" promptText="lastname">
               <font>
                  <Font name="Arial Rounded MT Bold" size="14.0" />
               </font></TextField>
            <TextField fx:id="txtAge" promptText="age">
               <font>
                  <Font name="Arial Rounded MT Bold" size="14.0" />
               </font></TextField>
            <TextField fx:id="txtAddress" promptText="address">
               <font>
                  <Font name="Arial Rounded MT Bold" size="14.0" />
               </font></TextField>
            <TextField fx:id="txtEmail" promptText="email">
               <font>
                  <Font name="Arial Rounded MT Bold" size="14.0" />
               </font></TextField>
            <PasswordField fx:id="txtPassword" promptText="password">
               <font>
                  <Font name="Arial Rounded MT Bold" size="14.0" />
               </font></PasswordField>
         </children>
      </VBox>
   </children>
</AnchorPane>
